# Model Provider (choose one)
MODEL_PROVIDER=anthropic  # or openai, bedrock

# Anthropic (Recommended for non-AWS users)
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# OpenAI (Alternative)
# OPENAI_API_KEY=your_openai_api_key_here

# AWS Bedrock (Alternative - requires AWS credentials)
# AWS_PROFILE=your_aws_profile

# Deepgram - Speech & Audio Processing
DEEPGRAM_API_KEY=your_deepgram_api_key_here
DEEPGRAM_DEFAULT_MODEL=nova-3
DEEPGRAM_DEFAULT_LANGUAGE=en

# HubSpot - CRM Operations
HUBSPOT_API_KEY=your_hubspot_api_key_here
HUBSPOT_DEFAULT_LIMIT=100

# Microsoft Teams - Notifications
TEAMS_WEBHOOK_URL=your_teams_webhook_url_here

# Optional: Observability (Langfuse)
# LANGFUSE_HOST=https://cloud.langfuse.com
# LANGFUSE_PUBLIC_KEY=your_public_key
# LANGFUSE_SECRET_KEY=your_secret_key
